   -
     name : _ModificationsRépertoire
     procedure_id : 1208674410652207155
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //_ModificationsRépertoire (<sNomRépertoire> est chaîne, <sNomFichier> est chaîne, <nTypeChangement> est entier, <sParamètre>)
      //
      // Paramètres :
      //	sNomRépertoire (chaîne ANSI) : <indiquez ici le rôle de sElement>
      //	sNomFichier (chaîne ANSI) : <indiquez ici le rôle de sParamètre>
      //	nTypeChangement (entier) : <indiquez ici le rôle de nTypeChangement>
      //	sParamètre : <indiquez ici le rôle de sParamètre>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE _ModificationsRépertoire(sNomRépertoire est une chaîne, sNomFichier est une chaîne, nTypeChangement est un entier, sParamètre)
      
      sMessage est une chaine
      sElément est une chaine = sNomRépertoire + ["\"] + sNomFichier
      
      selon nTypeChangement
      	cas fsSuppressionFichier
      		sMessage = gImage(IMG_SUPPR) + " " + ChaîneConstruit("L'élément (fichier ou répertoire) <%1> a été supprimé", sElément)
      				
      	cas fsCréationFichier		
      		// On teste l'existence du fichier, s'il n'existe pas, c'est un répertoire
      		SI fFichierExiste(sElément) = Faux ALORS
      			// C'est un répertoire
      			sMessage = gImage(IMG_AJOUT) + " " + ChaîneConstruit("Le répertoire <%1> a été ajouté", sElément)	
      		SINON
      			sMessage = gImage(IMG_AJOUT) + " " + ChaîneConstruit("Le fichier <%1> a été ajouté", sElément)
      		FIN
      	cas fsRenommage
      		// On teste l'existence du fichier, s'il n'existe pas, c'est un répertoire
      		SI fFichierExiste(sElément) = Faux ALORS
      			// C'est un répertoire
      			sMessage = gImage(IMG_RENAME) + " " + ChaîneConstruit("Le répertoire <%1> a été renommé en <%2>", sParamètre, sElément)
      		SINON
      			
      			sMessage = gImage(IMG_RENAME) + " " + ChaîneConstruit("Le fichier <%1> a été renommé en <%2>", sParamètre, sElément)
      		FIN
      		
      	cas fsModificationFichier
      		// On teste l'existence du fichier, s'il n'existe pas, c'est un répertoire
      		SI fFichierExiste(sElément) = Faux ALORS
      			// C'est un répertoire
      			sMessage = gImage(IMG_MODIF) + " " + ChaîneConstruit("Le répertoire <%1> a été modifié", sElément)	
      		SINON
      			sMessage = gImage(IMG_MODIF) + " " + ChaîneConstruit("Le fichier <%1> a été modifié", sElément)
      		FIN
      
      	AUTRE CAS
      		dbgassertion(faux,"Notification inconnue")
      		retour
      FIN
      
      
      AjouteLog(sMessage)
     type : 458752
